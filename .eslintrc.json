{
	"env": {
		"browser": true,
		"es6": true,
		"jasmine": true,
		"jest": true,
		"mocha": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"plugins": [
		"eslint-plugin-react",
		"eslint-plugin-import",
		"eslint-plugin-jsx-a11y",
		"eslint-plugin-react",
		"eslint-plugin-react-hooks"
	],
	"rules": {
		"accessor-pairs": 1,
		"array-bracket-newline": 1,
		"array-bracket-spacing": [
			2,
			"always",
			{
				"arraysInArrays": false,
				"objectsInArrays": false,
				"singleValue": false
			}
		],
		"array-callback-return": 1,
		"array-element-newline": 1,
		"arrow-body-style": 1,
		"arrow-parens": 1,
		"arrow-spacing": 1,
		"block-scoped-var": 1,
		"block-spacing": 1,
		"brace-style": 1,
		"callback-return": 1,
		"camelcase": 1,
		"capitalized-comments": 1,
		"class-methods-use-this": 1,
		"comma-dangle": 2,
		"comma-spacing": 1,
		"comma-style": 1,
		"complexity": 1,
		"computed-property-spacing": 1,
		"consistent-return": 1,
		"consistent-this": 1,
		"constructor-super": 1,
		"curly": 1,
		"default-case": 1,
		"default-param-last": 1,
		"dot-location": 1,
		"dot-notation": 1,
		"eol-last": 1,
		"eqeqeq": 1,
		"func-call-spacing": 1,
		"func-name-matching": 1,
		"func-names": 1,
		"func-style": 1,
		"function-call-argument-newline": 1,
		"function-paren-newline": 1,
		"generator-star-spacing": [
			"error",
			"after"
		],
		"global-require": 1,
		"guard-for-in": 1,
		"handle-callback-err": 1,
		"id-blacklist": 1,
		"id-length": 1,
		"id-match": 1,
		"implicit-arrow-linebreak": 1,
		"indent": 2,
		"indent-legacy": 2,
		"init-declarations": 1,
		"jsx-quotes": 1,
		"key-spacing": [
			2,
			{
				"afterColon": true,
				"align": {
					"on": "colon"
				},
				"beforeColon": false
			}
		],
		"keyword-spacing": [
			2,
			{
				"after": true,
				"before": true
			}
		],
		"line-comment-position": 1,
		"linebreak-style": 1,
		"lines-around-comment": 1,
		"lines-around-directive": 1,
		"lines-between-class-members": 1,
		"max-classes-per-file": 1,
		"max-depth": 1,
		"max-len": 1,
		"max-lines": 1,
		"max-lines-per-function": 1,
		"max-nested-callbacks": 1,
		"max-params": 1,
		"max-statements": 1,
		"max-statements-per-line": 1,
		"multiline-comment-style": [
			2,
			"separate-lines"
		],
		"multiline-ternary": 1,
		"new-cap": 1,
		"new-parens": 1,
		"newline-after-var": 1,
		"newline-before-return": 1,
		"newline-per-chained-call": 1,
		"no-alert": 1,
		"no-array-constructor": 1,
		"no-await-in-loop": 1,
		"no-bitwise": 1,
		"no-buffer-constructor": 1,
		"no-caller": 1,
		"no-case-declarations": 1,
		"no-catch-shadow": 1,
		"no-class-assign": 1,
		"no-confusing-arrow": 1,
		"no-console": 0,
		"no-const-assign": 1,
		"no-continue": 1,
		"no-delete-var": 1,
		"no-div-regex": 1,
		"no-dupe-class-members": 1,
		"no-duplicate-imports": 2,
		"no-else-return": 1,
		"no-empty-function": 1,
		"no-empty-pattern": 1,
		"no-eq-null": 1,
		"no-eval": 1,
		"no-extend-native": 1,
		"no-extra-bind": 1,
		"no-extra-label": 1,
		"no-extra-parens": 1,
		"no-fallthrough": 1,
		"no-floating-decimal": 1,
		"no-global-assign": 1,
		"no-implicit-coercion": 1,
		"no-implicit-globals": 1,
		"no-implied-eval": 1,
		"no-import-assign": 1,
		"no-inline-comments": 0,
		"no-invalid-this": 1,
		"no-iterator": 1,
		"no-label-var": 1,
		"no-labels": 1,
		"no-lone-blocks": 1,
		"no-lonely-if": 0,
		"no-loop-func": 1,
		"no-magic-numbers": 1,
		"no-mixed-operators": 1,
		"no-mixed-requires": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-multi-assign": 1,
		"no-multi-spaces": 1,
		"no-multi-str": 1,
		"no-multiple-empty-lines": 1,
		"no-native-reassign": 1,
		"no-negated-condition": 2,
		"no-negated-in-lhs": 1,
		"no-nested-ternary": 0,
		"no-new": 1,
		"no-new-func": 1,
		"no-new-object": 1,
		"no-new-require": 1,
		"no-new-symbol": 1,
		"no-new-wrappers": 1,
		"no-octal": 1,
		"no-octal-escape": 1,
		"no-param-reassign": 1,
		"no-path-concat": 1,
		"no-plusplus": 1,
		"no-process-env": 1,
		"no-process-exit": 1,
		"no-proto": 1,
		"no-redeclare": 1,
		"no-restricted-globals": 1,
		"no-restricted-imports": 1,
		"no-restricted-modules": 1,
		"no-restricted-properties": 1,
		"no-restricted-syntax": 1,
		"no-return-assign": 1,
		"no-return-await": 1,
		"no-script-url": 1,
		"no-self-assign": 1,
		"no-self-compare": 1,
		"no-sequences": 1,
		"no-shadow": 1,
		"no-shadow-restricted-names": 1,
		"no-spaced-func": 1,
		"no-sync": 1,
		"no-tabs": 1,
		"no-template-curly-in-string": 1,
		"no-ternary": 1,
		"no-this-before-super": 1,
		"no-throw-literal": 1,
		"no-trailing-spaces": 2,
		"no-undef": 1,
		"no-undef-init": 1,
		"no-undefined": 1,
		"no-underscore-dangle": 1,
		"no-unmodified-loop-condition": 1,
		"no-unneeded-ternary": 1,
		"no-unused-expressions": 1,
		"no-unused-labels": 1,
		"no-unused-vars": 1,
		"no-use-before-define": 1,
		"no-useless-call": 1,
		"no-useless-catch": 1,
		"no-useless-computed-key": 2,
		"no-useless-concat": 1,
		"no-useless-constructor": 2,
		"no-useless-escape": 2,
		"no-useless-rename": 2,
		"no-useless-return": 2,
		"no-var": 2,
		"no-void": 1,
		"no-warning-comments": 1,
		"no-whitespace-before-property": 2,
		"no-with": 1,
		"nonblock-statement-body-position": 1,
		"object-curly-newline": [
			2,
			{
				"ObjectExpression": "never",
				"ObjectPattern": "always",
				"multiline": true
			}
		],
		"object-curly-spacing": [
			2,
			"always",
			{
				"arraysInObjects": true,
				"objectsInObjects": true
			}
		],
		"object-property-newline": [
			2,
			"always",
			{
				"allowAllPropertiesOnSameLine": false
			}
		],
		"object-shorthand": 1,
		"one-var": 1,
		"one-var-declaration-per-line": 1,
		"operator-assignment": 1,
		"operator-linebreak": 1,
		"padded-blocks": 1,
		"padding-line-between-statements": 1,
		"prefer-arrow-callback": 1,
		"prefer-const": 2,
		"prefer-destructuring": [
			"error",
			{
				"AssignmentExpression": {
					"array": true,
					"object": false
				},
				"VariableDeclarator": {
					"array": false,
					"object": true
				}
			},
			{
				"enforceForRenamedProperties": false
			}
		],
		"prefer-named-capture-group": 1,
		"prefer-numeric-literals": 1,
		"prefer-object-spread": 2,
		"prefer-promise-reject-errors": 1,
		"prefer-reflect": 1,
		"prefer-regex-literals": 1,
		"prefer-rest-params": 1,
		"prefer-spread": 2,
		"prefer-template": 2,
		"quote-props": 1,
		"quotes": [
			2,
			"backtick",
			{
				"allowTemplateLiterals": true,
				"avoidEscape": true
			}
		],
		"radix": 1,
		"require-await": 1,
		"require-jsdoc": 1,
		"require-unicode-regexp": 1,
		"require-yield": 2,
		"rest-spread-spacing": [
			2,
			"never"
		],
		"semi": 1,
		"semi-spacing": 1,
		"semi-style": 1,
		"sort-imports": 2,
		"sort-keys": 1,
		"sort-vars": 1,
		"space-before-blocks": 1,
		"space-before-function-paren": 1,
		"space-in-parens": 1,
		"space-infix-ops": 1,
		"space-unary-ops": 1,
		"spaced-comment": 1,
		"strict": 0,
		"switch-colon-spacing": 1,
		"symbol-description": 2,
		"template-curly-spacing": 1,
		"template-tag-spacing": 1,
		"unicode-bom": 1,
		"valid-jsdoc": 1,
		"vars-on-top": 1,
		"wrap-iife": 1,
		"wrap-regex": 1,
		"yield-star-spacing": 2,
		"yoda": 1
	}
}